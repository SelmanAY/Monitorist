{
  "senderType": "Monitorist.Pump.GraphiteSender.GraphiteSender",
  "senderConfiguration": {
    "server": "graphite.linux.netone.net.tr",
    "port": 2003,
    "metricPrefix": "turknet.test",
    "protocol": "udp",
    "batchSize": "100",
    "transformations": [
      {
        "regex": "physicaldisk\\(.* (.*)\\:\\)",
        "replace": "physicaldisk.#{CAPTUREGROUP}-drive"
      },
      {
        "regex": "^\\\\",
        "replace": ""
      },
      {
        "regex": "\\\\",
        "replace": ""
      },
      {
        "regex": "/",
        "replace": "-"
      },
      {
        "regex": ":",
        "replace": "."
      },
      {
        "regex": "\\(",
        "replace": ""
      },
      {
        "regex": "\\.",
        "replace": ""
      },
      {
        "regex": " ",
        "replace": ""
      },
      {
        "regex": "%",
        "replace": ""
      }
    ]
  }
}